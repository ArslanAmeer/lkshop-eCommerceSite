@model FYProject1Classes.ProductMgmt.Camera

@{
    ViewBag.Title = Model.Title + " Details";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
    var svg = " data:image/gif;base64,";
}
@section myStyles{
    <style type="text/css">
        pre {
            white-space: pre-wrap; /* Since CSS 2.1 */
            word-wrap: break-word; /* Internet Explorer 5.5+ */
        }

        @@media screen and (max-width: 992px) {
            img {
                margin-left: 30vw;
            }
        }

        @@media screen and (max-width: 767px) {
            dd, dt {
                text-align: center;
            }

                dd:after {
                    content: "";
                    display: block;
                    width: 100%;
                    height: 3px;
                    /*color: darkgrey;*/
                    background-color: lightgray;
                }

            dt {
                text-decoration: underline;
            }
        }
    </style>
}
<h2 class="list-group-item-heading text-center notextselect">@Html.DisplayFor(model => model.Title)</h2>

<div class="col-lg-9">
    <h4>@Html.DisplayFor(model => model.Title) - <em>@Html.DisplayFor(model => model.Kit) </em></h4>
    <hr />
    <div class="col-lg-3 col-md-2 col-sm-12">
        @foreach (var item in Model.Images)
        {
            <div>
                @* 4k Image URL: https://hdqwalls.com/wallpapers/injustice-2-superman-image.jpg *@
                <img class="b-lazy" data-toggle="magnify" src=@svg data-src="@Url.Content(item.Image_Url)" alt="images" height="150" width="150" />
            </div>
            <hr />
            <br />
        }
    </div>

    <div class="col-lg-9 col-md-7 col-sm-12">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Title)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Title)
            </dd>

            @if (Model.Stock != null && Model.Stock > 0)
            {
                <dt style="color: green">
                    Stock Available
                </dt>
                <dd style="color: green">
                    @Html.DisplayFor(model => model.Stock)
                </dd>
            }
